{
  "name": "fepfan",
  "version": "1.1.0",
  "description": "是否有过当你看到比较优秀的前端资源时，由于没有时间来的及收录，过段时间等需要的时，却翻来覆去不知道去哪了～～ 前端导航站，收集前端业内优秀技术博客、框架，方便快速寻找优秀资源，～～宝宝再也不用担心查找资源了～～",
  "main": "index.js",
  "scripts": {
    "commitmsg": "validate-commit-msg",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "dev": "cross-env webpack-dev-server --colors --progress",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack --config webpack.config.js",
    "build": "rm -rf ./dist && cross-env NODE_ENV=production webpack --config webpack.config.js --colors"
  },
  "author": "@pfan",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.0.1",
    "axios": "^0.16.1",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-latest": "^6.24.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^2.0.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "glob": "^7.1.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.13.4",
    "imagemin-webpack-plugin": "^1.4.4",
    "leancloud-storage": "^2.3.0",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.5.2",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^2.0.5",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "validate-commit-msg": "^2.12.2",
    "vue-axios": "^2.0.2",
    "vue-html": "0.3.0",
    "vue-loader": "11.3.4",
    "vue-resource": "^1.3.1",
    "vue-router": "^2.5.3",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.0",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "vue": "^2.3.0"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "build",
        "ci",
        "chore",
        "revert"
      ],
      "scope": {
        "required": false,
        "allowed": [
          "*"
        ],
        "validate": false,
        "multiple": false
      },
      "warnOnFail": false,
      "maxSubjectLength": 1000,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": "",
      "autoFix": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pfan123/front-end-navigator.git"
  },
  "bugs": {
    "url": "https://github.com/pfan123/front-end-navigator/issues"
  },
  "homepage": "https://github.com/pfan123/front-end-navigator#readme"
}
